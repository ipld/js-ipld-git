  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipld/js-ipld-git/issues"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "homepage": "https://github.com/ipld/js-ipld-git",
  "dependencies": {
    "async": "^2.6.0",
    "cids": "~0.5.2",
    "multicodec": "~0.2.5",
    "multihashes": "~0.4.12",
    "multihashing-async": "~0.4.7",
    "smart-buffer": "^4.0.0",
    "traverse": "~0.6.6"
  },
  "devDependencies": {
    "aegir": "^12.4.0",
    "chai": "^4.1.2",
    "deep-freeze": "0.0.1",
    "dirty-chai": "^2.0.1",
    "garbage": "0.0.0",
    "ipfs-block": "~0.6.1"
  },
